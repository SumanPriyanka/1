public class EvenOrOdd {
    public static String checkEvenOrOdd(int number) {
        if (number % 2 == 0) {
            return "Even";
        } else {
            return "Odd";
        }
    }

    public static void main(String[] args) {
        int number = 7;
        System.out.println(checkEvenOrOdd(number));
    }
}

